{
  "manifest_version": 2,
  "name": "SEOclerks Affiliate link builder",
  "short_name": "1Click SEOclerks",
  "description": "Build your SEOclerks affiliate link in 1 click",
  "version": "1.0",

  "permissions": [
  "http://*/*",
  "https://*/*",
  "tabs"
 ],
  "browser_action": {
    "default_icon" : "img/icon.png",
    "default_popup": "popup.html"
 },
 "icons": {
  "128" : "img/icon.png"
 },
 "content_scripts": [ {
    "matches": ["http://*/*"],
    "css": ["css/materialize.min.css"],
    "js": ["js/jquery.js"]
} ]
}
